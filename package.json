{
  "name": "wof",
  "version": "2.0.0",
  "description": "Configurable wheel of fortune with specific angle, build with ES6, Webpack and SASS",
  "main": "build/wof.min.js",
  "npmName": "wof",
  "npmFileMap": [
    {
      "basePath": "build",
      "files": [
        "**/*"
      ]
    }
  ],
  "scripts": {
    "build": "webpack --env dev && webpack --env build",
    "dev": "webpack --progress --colors --watch --env dev",
    "test": "mocha --require babel-core/register --colors ./test/*.spec.js",
    "test:watch": "mocha --require babel-core/register --colors -w ./test/*.spec.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/muhibbudins/wheel-of-fortune.git"
  },
  "keywords": [
    "webpack",
    "es6",
    "library",
    "universal",
    "umd",
    "commonjs",
    "wheel",
    "fortune",
    "wheel of fortune",
    "wheel of destiny",
    "destiny"
  ],
  "author": "Muhibbudin Suretno <muhibbudinsuretno1@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/muhibbudins/wheel-of-fortune/issues"
  },
  "homepage": "https://github.com/muhibbudins/wheel-of-fortune",
  "devDependencies": {
    "axios": "^0.18.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.1.2",
    "css-loader": "^0.28.11",
    "cypress": "^3.4.1",
    "domtastic": "^0.15.5",
    "eslint": "^4.13.1",
    "eslint-loader": "^1.9.0",
    "lodash": "^4.17.5",
    "mocha": "^4.0.1",
    "node-sass": "^4.12.0",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.3",
    "svg.js": "^2.6.4",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "webpack": "^3.10.0",
    "yargs": "^10.0.3",
    "zero-chance": "^0.2.0"
  }
}
